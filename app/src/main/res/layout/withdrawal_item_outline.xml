                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/guideline11"
                    app:layout_constraintStart_toStartOf="@+id/txt_response_message"
                    app:layout_constraintTop_toBottomOf="@+id/txt_date"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/txt_authorization_code"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="@string/authorization_code"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/guideline11"
                    app:layout_constraintStart_toStartOf="@+id/txt_retrieval_reference"
                    app:layout_constraintTop_toBottomOf="@+id/txt_retrieval_reference"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/tranasaction_type_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/t_type_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_transaction_type"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/txt_transaction_type"
                    app:layout_constraintTop_toTopOf="@+id/txt_transaction_type" />

                <TextView
                    android:id="@+id/Transaction_status_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/t_status_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_transaction_status"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/txt_transaction_status"
                    app:layout_constraintTop_toTopOf="@+id/txt_transaction_status" />

                <TextView
                    android:id="@+id/Transaction_reference_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/t_reference_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_transaction_reference"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/txt_transaction_reference"
                    app:layout_constraintTop_toTopOf="@+id/txt_transaction_reference" />

                <TextView
                    android:id="@+id/Amount_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/amount_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_amount"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/guideline11"
                    app:layout_constraintTop_toTopOf="@+id/txt_amount" />

                <TextView
                    android:id="@+id/Beneficiary_name_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/b_name_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_beneficiary_name"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/guideline11"
                    app:layout_constraintTop_toTopOf="@+id/txt_beneficiary_name" />

                <TextView
                    android:id="@+id/Beneficiary_bank_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/b_bank_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_beneficiary_bank"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/guideline11"
                    app:layout_constraintTop_toTopOf="@+id/txt_beneficiary_bank" />

                <TextView
                    android:id="@+id/Provider_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/pr_session_id_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_provider_reference_or_session_id"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/txt_provider_reference_or_session_id"
                    app:layout_constraintTop_toTopOf="@+id/txt_provider_reference_or_session_id" />

                <TextView
                    android:id="@+id/Agent_name_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/ag_name_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_agent_name"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/guideline11"
                    app:layout_constraintTop_toTopOf="@+id/txt_agent_name" />

                <TextView
                    android:id="@+id/Terminal_ID_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/t_id_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_terminal_id"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/guideline11"
                    app:layout_constraintTop_toTopOf="@+id/txt_terminal_id" />

                <TextView
                    android:id="@+id/Serial_no_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/s_no_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_serial_no"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/txt_serial_no"
                    app:layout_constraintTop_toTopOf="@+id/txt_serial_no" />

                <TextView
                    android:id="@+id/Response_code_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="@string/r_code_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/txt_response_code"
                    app:layout_constraintTop_toBottomOf="@+id/Serial_no_ans"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/Response_message_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/r_mess_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_response_message"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/guideline11"
                    app:layout_constraintTop_toTopOf="@+id/txt_response_message" />

                <TextView
                    android:id="@+id/Date_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/date_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_date"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/txt_date"
                    app:layout_constraintTop_toTopOf="@+id/txt_date" />

                <TextView
                    android:id="@+id/Retrieval_reference_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/r_r_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_retrieval_reference"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/txt_retrieval_reference"
                    app:layout_constraintTop_toTopOf="@+id/txt_retrieval_reference" />

                <TextView
                    android:id="@+id/Authorization_code_ans"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/a_code_response"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_authorization_code"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/txt_authorization_code"
                    app:layout_constraintTop_toTopOf="@+id/txt_authorization_code" />

            </androidx.constraintlayout.widget.ConstraintLayout>


        </ScrollView>


    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>